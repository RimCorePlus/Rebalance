<Patch>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>True</enabled>
		<label>double gauranlen pruning speed</label>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="Plant_TreeGauranlen"]/comps/li[@Class="CompProperties_TreeConnection"]/connectionStrengthGainPerHourPruningBase</xpath>
				<value>
					<connectionStrengthGainPerHourPruningBase>0.2</connectionStrengthGainPerHourPruningBase>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>