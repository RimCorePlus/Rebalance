<Patch>
	<Operation Class="ModSettingsFramework.PatchOperationModOption">
		<defaultValue>false</defaultValue>
		<label>Double child learning rate factor</label>
		<id>BL_RebalanceChildLearningFactor</id>
		<category>BL_RebalanceBiotech</category>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>//StatDef[defName="LearningRateFactor"]/defaultBaseValue/text()</xpath>
				<value>2</value>
			</li>
		</operations>
	</Operation>
</Patch>