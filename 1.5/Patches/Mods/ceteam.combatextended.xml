<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Combat Extended</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			<!--removes industrial basic tags from backpacks because literally every pawn has one lol-->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[@ParentName="ApparelCarryGearBase"]/apparel/tags/li[text()="IndustrialBasic" or text()="Neolithic"]</xpath>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>