<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dusk Armory</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			<!--hard removes most of the bloat-->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[starts-with(defName, "DF_Dusk_")]/apparel/tags</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[starts-with(defName, "DF_Dusk_")]/apparel/defaultOutfitTags</xpath>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>