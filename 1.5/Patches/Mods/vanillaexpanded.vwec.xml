<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Weapons Expanded - Coilguns</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			<!--no enemies spawning with coilguns-->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[starts-with(defName, "VWE_Gun_Gauss")]/weaponTags/li[text()="SpacerGun"]</xpath>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>