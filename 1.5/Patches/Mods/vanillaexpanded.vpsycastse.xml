<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Psycasts Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			<!--removes psycaster raids so deserters in spacer armor aren't attacking my day 20 colony-->
				<li Class="PatchOperationRemove">
					<xpath>Defs/RaidStrategyDef[defName="VPE_ImmediateAttack_Psycasters"]</xpath>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>