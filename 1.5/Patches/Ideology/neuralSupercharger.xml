<Patch>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>True</enabled>
		<label>reduce neural supercharger recharge time</label>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>//ThingDef[defName="NeuralSupercharger"]/comps/li[@Class="CompProperties_NeuralSupercharger"]/ticksToRecharge</xpath>
				<value>
					<ticksToRecharge>20000</ticksToRecharge>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>