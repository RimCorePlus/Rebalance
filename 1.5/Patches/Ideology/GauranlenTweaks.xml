<Patch>
	<Operation Class="XmlExtensions.UseSetting">
		<modId>friedrice</modId>
		<key>GauranlenPruningSpeed</key>
		<defaultValue>0.2</defaultValue>
		<apply>
			<!--Increases tree pruning speed: 0.01 -> 0.02-->
			<Operation Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="Plant_TreeGauranlen"]/comps/li[@Class="CompProperties_TreeConnection"]/connectionStrengthGainPerHourPruningBase</xpath>
				<value>
					<connectionStrengthGainPerHourPruningBase>{GauranlenPruningSpeed}</connectionStrengthGainPerHourPruningBase>
				</value>
			</Operation>
		</apply>
	</Operation>
</Patch>