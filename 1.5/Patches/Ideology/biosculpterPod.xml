<Patch>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>false</enabled>
		<label>Decrease biosculpter age reversal cycle time to 5 days</label>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>//ThingDef[defName="BiosculpterPod"]/comps/li[@Class="CompProperties_BiosculpterPod_AgeReversalCycle"]/durationDays/text()</xpath>
				<value>5</value>
			</li>
		</operations>
	</Operation>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>false</enabled>
		<label>Decrease biosculpter regeneration cycle time to 10 days</label>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>//ThingDef[defName="BiosculpterPod"]/comps/li[compClass="CompBiosculpterPod_RegenerationCycle"]/durationDays/text()</xpath>
				<value>10</value>
			</li>
		</operations>
	</Operation>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>false</enabled>
		<label>Decrease biosculpter medic cycle time to 2 days</label>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>//ThingDef[defName="BiosculpterPod"]/comps/li[compClass="CompBiosculpterPod_MedicCycle"]/durationDays/text()</xpath>
				<value>2</value>
			</li>
		</operations>
	</Operation>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>false</enabled>
		<label>Decrease biosculpter pleasure cycle time to 1 day</label>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>//ThingDef[defName="BiosculpterPod"]/comps/li[@Class="CompProperties_BiosculpterPod_PleasureCycle"]/durationDays/text()</xpath>
				<value>1</value>
			</li>
		</operations>
	</Operation>
</Patch>