<Patch>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>friedrice</modId>
		<key>preferredApparel</key>
		<defaultValue>true</defaultValue>
		<caseTrue>
			<Operation Class="PatchOperationReplace"> <!--Disable preferred apparel when ideologions are generated-->
					<xpath>Defs/MemeDef/selectOneOrNone/noneChance</xpath>
					<value>
						<noneChance>1</noneChance>
					</value>
			</Operation>
		</caseTrue>
	</Operation>
</Patch>