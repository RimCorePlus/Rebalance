<Patch>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>friedrice</modId>
		<key>preferredApparel</key>
		<defaultValue>true</defaultValue>
		<caseTrue>
			<!--Disable preferred apparel when ideologions are generated-->
			<Operation Class="PatchOperationReplace">
				<xpath>Defs/MemeDef/selectOneOrNone/noneChance</xpath>
				<value>
					<noneChance>1</noneChance>
				</value>
			</Operation>
		</caseTrue>
	</Operation>
</Patch>