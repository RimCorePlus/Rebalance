<Patch>
	<Operation Class="XmlExtensions.UseSetting">
		<modId>friedrice</modId>
		<key>AgeReversalCycle</key>
		<defaultValue>5</defaultValue>
		<apply>
			<Operation Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="BiosculpterPod"]/comps/li[@Class="CompProperties_BiosculpterPod_AgeReversalCycle"]/durationDays</xpath>
				<value>
					<durationDays>{AgeReversalCycle}</durationDays>
				</value>
			</Operation>
		</apply>
	</Operation>
	<Operation Class="XmlExtensions.UseSetting">
		<modId>friedrice</modId>
