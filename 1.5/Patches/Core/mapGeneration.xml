<Patch>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>true</enabled>
		<label>Boreal forests are not marshes</label>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>//BiomeDef[defName="BorealForest"]/terrainPatchMakers</xpath>
				<value>
					<terrainPatchMakers>
						<li>
							<perlinFrequency>0.015</perlinFrequency>
							<thresholds>
								<li>
									<terrain>MossyTerrain</terrain>
									<min>0.0</min>
									<max>0.64</max>
								</li>
								<li>
									<terrain>Marsh</terrain>
									<min>0.64</min>
									<max>0.88</max>
								</li>
								<li>
									<terrain>WaterShallow</terrain>
									<min>0.88</min>
									<max>0.98</max>
								</li>
								<li>
									<terrain>WaterDeep</terrain>
									<min>0.98</min>
									<max>999</max>
								</li>
							</thresholds>
						</li>
					</terrainPatchMakers>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>