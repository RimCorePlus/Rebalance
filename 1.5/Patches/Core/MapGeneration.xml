<Patch>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>friedrice</modId>
		<key>UnbogBoreal</key>
		<defaultValue>true</defaultValue>
		<caseTrue>
			<Operation Class="PatchOperationReplace">
				<xpath>Defs/BiomeDef[defName="BorealForest"]/terrainPatchMakers</xpath>
				<value>
					<terrainPatchMakers>
						<li>
							<perlinFrequency>0.015</perlinFrequency>
							<thresholds>
								<li>
									<terrain>MossyTerrain</terrain>
									<min>0.0</min>
									<max>0.64</max>
								</li>
								<li>
									<terrain>Marsh</terrain>
									<min>0.64</min>
									<max>0.88</max>
								</li>
								<li>
									<terrain>WaterShallow</terrain>
									<min>0.88</min>
									<max>0.98</max>
								</li>
								<li>
									<terrain>WaterDeep</terrain>
									<min>0.98</min>
									<max>999</max>
								</li>
							</thresholds>
						</li>
					</terrainPatchMakers>
				</value>
			</Operation>
		</caseTrue>
	</Operation>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>friedrice</modId>
		<key>RemoveCompactedMachinery</key>
		<defaultValue>true</defaultValue>
		<caseTrue>
			<Operation Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="MineableComponentsIndustrial"]/building/mineableScatterCommonality</xpath>
				<value>
					<mineableScatterCommonality>0</mineableScatterCommonality>
				</value>
			</Operation>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/GenStepDef/genStep/mineables/li[text()="MineableComponentsIndustrial"]</xpath>
			</Operation>
		</caseTrue>
	</Operation>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>friedrice</modId>
		<key>RemoveCompactedPlasteel</key>
		<defaultValue>false</defaultValue>
		<caseTrue>
			<Operation Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="MineablePlasteel"]/building/mineableScatterCommonality</xpath>
				<value>
					<mineableScatterCommonality>0</mineableScatterCommonality>
				</value>
			</Operation>
			<Operation Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="Plasteel"]/deepCommonality</xpath>
				<value>
					<deepCommonality>0</deepCommonality>
				</value>
			</Operation>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/GenStepDef/genStep/mineables/li[text()="MineablePlasteel"]</xpath>
			</Operation>
		</caseTrue>
	</Operation>
</Patch>