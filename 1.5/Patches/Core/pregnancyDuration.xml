<Patch>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>true</enabled>
		<label>longer human pregnancy duration</label>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>//ThingDef[defName="Human"]/race/gestationPeriodDays/text()</xpath>
				<value>45</value>
			</li>
		</operations>
	</Operation>
</Patch>