<Patch>
	<Operation Class="XmlExtensions.UseSetting">
		<modId>friedrice</modId>
		<key>PregnancyDuration</key>
		<defaultValue>45</defaultValue>
		<apply>
			<Operation Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="Human"]/race/gestationPeriodDays</xpath>
				<value>
					<gestationPeriodDays>{PregnancyDuration}</gestationPeriodDays>
				</value>
			</Operation>
		</apply>
	</Operation>
</Patch>