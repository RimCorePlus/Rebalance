<Patch>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>friedrice</modId>
		<key>RemoveDarkMelanin</key>
		<defaultValue>true</defaultValue>
		<caseTrue>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/GeneDef[defName="Skin_Melanin4" or defName="Skin_Melanin5" or defName="Skin_Melanin6" or defName="Skin_Melanin7" or defName="Skin_Melanin8" or defName="Skin_Melanin9"]</xpath>
			</Operation>
		</caseTrue>
	</Operation>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>friedrice</modId>
		<key>RemoveFat</key>
		<defaultValue>true</defaultValue>
		<caseTrue>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/BackstoryDef[bodyTypeFemale="Fat"]/bodyTypeFemale</xpath>
			</Operation>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/BackstoryDef[bodyTypeGlobal="Fat"]/bodyTypeGlobal</xpath>
			</Operation>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/BackstoryDef[bodyTypeMale="Fat"]/bodyTypeMale</xpath>
			</Operation>
		</caseTrue>
	</Operation>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>friedrice</modId>
		<key>RemoveHulk</key>
		<defaultValue>true</defaultValue>
		<caseTrue>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/BackstoryDef[bodyTypeFemale="Hulk"]/bodyTypeFemale</xpath>
			</Operation>
		</caseTrue>
	</Operation>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>friedrice</modId>
		<key>RemoveNarrow</key>
		<defaultValue>true</defaultValue>
		<caseTrue>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/HeadTypeDef[defName="Male_NarrowNormal" or defName="Male_NarrowPointy" or defName="Male_NarrowWide" or defName="Female_NarrowNormal" or defName="Female_NarrowPointy" or defName="Female_NarrowWide"]</xpath>
			</Operation>
			<Operation Class="PatchOperationFindMod">
				<mods>
					<li>Biotech</li>
				</mods>
				<match Class="PatchOperationSequence">
					<operations>
						<li Class="PatchOperationRemove">
							<xpath>Defs/HeadTypeDef[defName="Furskin_Narrow1" or defName="Furskin_Narrow2" or defName="Furskin_Narrow3"]</xpath>
						</li>
						<li Class="PatchOperationRemove">
							<xpath>Defs/GeneDef[defName="Furskin"]/forcedHeadTypes/li[text()="Furskin_Narrow1" or text()="Furskin_Narrow2" or text()="Furskin_Narrow3"]</xpath>
						</li>
					</operations>
				</match>
			</Operation>
		</caseTrue>
	</Operation>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>friedrice</modId>
		<key>RemoveFemaleWide</key>
		<defaultValue>true</defaultValue>
		<caseTrue>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/HeadTypeDef[defName="Female_AverageWide"]</xpath>
			</Operation>
		</caseTrue>
	</Operation>
</Patch>