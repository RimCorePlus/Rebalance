<Patch>
    <Operation Class="VFECore.PatchOperationToggableSequence">
        <enabled>false</enabled>
        <label>Stop spawning tribals with war veils</label>
        <operations>
            <li Class="PatchOperationRemove">
                <xpath>//ThingDef[defName="Apparel_WarVeil"]/apparel/defaultOutfitTags | //PawnKindDef/apparelRequired/li[text()="Apparel_WarVeil"]</xpath>
            </li>
        </operations>
    </Operation>
</Patch>