<Patch>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>false</enabled>
		<label>Boomalopes and boomrats bleed chemfuel</label>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>//ThingDef[defName="Boomalope" or defName="Boomrat"]/race</xpath>
				<value>
					<bloodDef>Filth_Fuel</bloodDef>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>