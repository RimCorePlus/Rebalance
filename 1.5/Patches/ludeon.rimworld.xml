<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<!--Prepatch tribal facions to allow for custom xenotypes-->
			<li Class="PatchOperationAdd">
				<xpath>Defs/FactionDef[@Name="TribeBase"]</xpath>
				<value>
					<xenotypeSet>
						<xenotypeChances></xenotypeChances>
					</xenotypeSet>
				</value>
			</li>
			<!--Clean up empty nodes-->
			<li Class="PatchOperationRemove">
				<xpath>Defs/ThingDef/comps[not(*)]</xpath>
			</li>
		</operations>
	</Operation>
</Patch>