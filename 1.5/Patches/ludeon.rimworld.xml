<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<!--Prepatch tribal facions to allow for custom xenotypes-->
			<li Class="PatchOperationAdd">
				<xpath>Defs/FactionDef[@Name="TribeBase"]</xpath>
				<value>
					<xenotypeSet>
						<xenotypeChances></xenotypeChances>
					</xenotypeSet>
				</value>
			</li>
			<!--Clean up empty nodes-->
			<li Class="PatchOperationRemove">
				<xpath>Defs/ThingDef/comps[not(*)]</xpath>
			</li>
		</operations>
	</Operation>
	<!--Temporary-->
	<Operation Class="PatchOperationInsert">
		<xpath>Defs/ThingDef[defName = "CryptosleepCasket"]/statBases</xpath>
		<value>
			<minifiedDef>MinifiedThing</minifiedDef>
			<thingCategories>
				<li>BuildingsMisc</li>
			</thingCategories>
		</value>
	</Operation>
	<Operation Class="PatchOperationInsert">
		<xpath>Defs/ThingDef[defName = "CryptosleepCasket"]/statBases/Flammability</xpath>
		<value>
			<Mass>75</Mass>
		</value>
	</Operation>
	<Operation Class="PatchOperationInsert">
		<xpath>Defs/ThingDef[defName = "AncientCryptosleepCasket"]/statBases</xpath>
		<value>
			<minifiedDef>MinifiedThing</minifiedDef>
			<thingCategories>
				<li>BuildingsMisc</li>
			</thingCategories>
		</value>
	</Operation>
	<Operation Class="PatchOperationInsert">
		<xpath>Defs/ThingDef[defName = "AncientCryptosleepCasket"]/statBases/Flammability</xpath>
		<value>
			<Mass>75</Mass>
		</value>
	</Operation>
</Patch>