<Patch>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>friedrice</modId>
		<key>removeYttakin</key>
		<defaultValue>true</defaultValue>
		<caseTrue>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/XenotypeDef/doubleXenotypeChances/Yttakin</xpath>
			</Operation>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/FactionDef/xenotypeSet/xenotypeChances/Yttakin</xpath>
			</Operation>
			<Operation Class="PatchOperationAdd">
				<xpath>Defs/XenotypeDef[defName="Yttakin"]</xpath>
				<value>
					<factionlessGenerationWeight>0</factionlessGenerationWeight>
				</value>
			</Operation>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/FactionDef[defName="PirateYttakin"]</xpath>
			</Operation>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/RulePackDef[defName="YttakinUtility" or
				defName="NamerFactionPirateYttakin" or
				defName="NamerSettlementPirateYttakin"]</xpath>
			</Operation>
			<Operation Class="PatchOperationRemove">
				<xpath>Defs/PawnKindDef[defName="Pirate_Yttakin" or defName="Scavenger_Yttakin" or defName="Drifter_Yttakin" or defName="Thrasher_Yttakin" or defName="Mercenary_Gunner_Yttakin" or defName="Mercenary_Heavy_Yttakin" or defName="Mercenary_Slasher_Yttakin" or defName="Mercenary_Sniper_Yttakin" or defName="Mercenary_Elite_Yttakin"]</xpath>
			</Operation>
		</caseTrue>
	</Operation>
</Patch>