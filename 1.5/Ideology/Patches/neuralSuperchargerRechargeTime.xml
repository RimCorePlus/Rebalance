<Patch>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>false</enabled>
		<label>Reduce neural supercharger recharge time to 8 hours</label>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>//ThingDef[defName="NeuralSupercharger"]/comps/li[@Class="CompProperties_NeuralSupercharger"]/ticksToRecharge/text()</xpath>
				<value>20000</value>
			</li>
		</operations>
	</Operation>
</Patch>