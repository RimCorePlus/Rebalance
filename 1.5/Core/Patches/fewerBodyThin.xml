<Patch>
	<Operation Class="VFECore.PatchOperationToggableSequence">
		<enabled>false</enabled>
        <label>Reduce thin body commonality</label>
        <operations>
            <li Class="PatchOperationRemove">
                <xpath>//BackstoryDef[bodyTypeFemale="Thin"]/bodyTypeFemale | //BackstoryDef[bodyTypeGlobal="Thin"]/bodyTypeGlobal | //BackstoryDef[bodyTypeMale="Thin"]/bodyTypeMale</xpath>
            </li>
        </operations>
    </Operation>
</Patch>