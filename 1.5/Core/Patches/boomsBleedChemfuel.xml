<Patch>
	<Operation Class="ModSettingsFramework.PatchOperationModOption">
		<defaultValue>false</defaultValue>
		<label>Boomalopes and boomrats bleed chemfuel</label>
		<id>BL_RebalanceBoomChemfuelBlood</id>
		<category>BL_RebalanceCoreAnimals</category>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>//ThingDef[defName="Boomalope" or defName="Boomrat"]/race</xpath>
				<value>
					<bloodDef>Filth_Fuel</bloodDef>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>